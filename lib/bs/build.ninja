  description = [34mBuilding[39m [2m${out}[22m
build src/CronExpression-NactPatterns.cmi : build_cmi src/CronExpression.mliast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
build src/CronExpression.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/CronExpression.re
build src/CronExpression.mlast.d : build_deps src/CronExpression.mlast

bs_package_flags = -bs-package-map NactPatterns
src_root_dir = /Users/ncthbrt/git/nact-patterns
bsc = /Users/ncthbrt/git/nact-patterns/node_modules/bs-platform/lib/bsc.exe
bsdep = /Users/ncthbrt/git/nact-patterns/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101 -warn-error +101
bsc_flags = -bs-suffix -nostdlib -I '/Users/ncthbrt/git/nact-patterns/node_modules/bs-platform/lib/ocaml' -no-alias-deps -color always
ppx_flags = 

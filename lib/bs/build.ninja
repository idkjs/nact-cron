  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
rule build_package
  command = ${bsc} -w -49 -no-alias-deps -c ${in}
  description = [34mBuilding[39m [2m${out}[22m
build NactPatterns.cmi : build_package NactPatterns.mlmap
build build.ninja : phony  ||  NactPatterns.cmi src/CronExpression.mlast.d src/CronExpression.mliast.d src/CronScheduler.mlast.d __tests__/CronExpressionTest.mlast.d

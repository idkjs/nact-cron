  bs_package_includes = $bs_package_includes $bs_package_dev_includes
  bsc_extra_includes = $bsc_group_1_includes
build src/CronScheduler.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/CronScheduler.re
build src/CronScheduler.mlast.d : build_deps src/CronScheduler.mlast
build src/CronScheduler-NactPatterns.cmj |  $src_root_dir/src/CronScheduler.bs.js src/CronScheduler-NactPatterns.cmi : build_cmj_cmi src/CronScheduler.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
rule build_ast_and_module_sets_from_rei
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx} ${ppx_flags} ${warnings} ${bsc_flags}  -c -o ${out} -bs-syntax-only -bs-binary-ast -intf ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/CronExpression.mliast : build_ast_and_module_sets_from_rei $src_root_dir/src/CronExpression.rei
build src/CronExpression.mliast.d : build_deps src/CronExpression.mliast
